{
  "routes": [
    {
      "canonical": "/",
      "path": "/",
      "strategy": "static",
      "template": "index.html",
      "handler": "index",
      "title": "pages.home.title"
    },
    {
      "canonical": "/blogs",
      "path": "/blogs",
      "strategy": "dynamic",
      "template": "blogs.html",
      "handler": "blogs",
      "title": "pages.blogs.title"
    },
    {
      "canonical": "/about",
      "path": "/about",
      "strategy": "static",
      "template": "about.html",
      "handler": "about",
      "title": "pages.about.title"
    },
    {
      "canonical": "/blogs/{slug}",
      "path": "/blogs/{slug}",
      "strategy": "static",
      "template": "blog-post.html",
      "handler": "blogpost"
    }
  ]
}
